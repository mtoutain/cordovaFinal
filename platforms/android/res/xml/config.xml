<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.tout0004.Quidditch" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Michel Toutain</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash density="land-hdpi" src="www/res/screens/screen-hdpi-landscape.png" />
    <splash density="land-xxhdpi" src="www/res/screens/screen-xxhdpi-landscape.png" />
    <splash density="land-mdpi" src="www/res/screens/screen-mdpi-landscape.png" />
    <splash density="land-xhdpi" src="www/res/screens/screen-xhdpi-landscape.png" />
    <splash density="land-xxxxhdpi" src="www/res/screens/screen-xxxhdpi-landscape.png" />
    <splash density="port-hdpi" src="www/res/screens/screen-hdpi-portrait.png" />
    <splash density="port-mdpi" src="www/res/screens/screen-mdpi-portrait.png" />
    <splash density="port-xhdpi" src="www/res/screens/screen-xhdpi-portrait.png" />
    <splash density="port-xxhdpi" src="www/res/screens/screen-xxhdpi-landscape.png" />
    <splash density="port-xxxhdpi" src="www/res/screens/screen-xxxhdpi-portrait.png" />
    <icon density="hdpi" src="www/res/icons/icon-72-hdpi.png" />
    <icon density="xhdpi" src="www/res/icons/icon-96-xhdpi.png" />
    <icon density="xxhdpi" src="www/res/icons/icon-144-xxhdpi.png" />
    <icon density="xxxhdpi" src="www/res/icons/icon-192-xxxhdpi.png" />
    <icon density="xxxxhdpi" src="www/res/icons/icon-512-xxxxhdpi.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
